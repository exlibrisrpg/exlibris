<% content_for(:title) do %><%= t(".title") %><% end %>

<h1 class="tw-text-4xl tw-font-semibold tw-text-primary-12"><%= t(".title") %></h1>

<%= render PaginationComponent.new(page: @page, path: ->(next_page) { entries_path(page: next_page) }) do |c| %>
  <% c.body do %>
    <%= render EntryComponent.with_collection(@page.records) %>
  <% end %>
<% end %>
